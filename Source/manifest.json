{
	"manifest_version": 2,
	"name": "Normalization To Not",
	"version": "1.0",
	"description": "Crosses out words that normalizes the Trump presidency and adds their abnormal counterpart.",
	"icons": { "16": "icon.png",
	     "48": "icon.png",
	     "64": "icon.png",
	    "128": "icon.png" },

	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["js/jquery-1.12.4.min.js", "content_script.js"],
			"run_at": "document_end"
		}
	]
}

